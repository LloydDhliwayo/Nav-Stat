<!DOCTYPE html>
<html>

<head>

    <title>Cloud_Navixy_CurrentStates</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous">
    <link rel="stylesheet" type="text/css" href="//cdn.datatables.net/1.10.23/css/jquery.dataTables.min.css">
    <link rel="stylesheet" type="text/css" href="//cdn.datatables.net/buttons/1.6.5/css/buttons.dataTables.min.css">

</head>

<body>




    <div class="col-md-6 offset-md-3 my-5">
        <div class="card">
            <div class="card-body">
                <h4 style="text-align:center">Customer Has
                    <%=numberofunits%> Units On Navixy Cloud</h4>
            </div>
        </div>
    </div>





    <div class="container">
        <table id="IDsTable" style="width:100%" class="cell-border compact stripe">
            <!-- <caption>Vehicle list For Searched Customer ID</caption> -->
            <thead>
                <tr>
                    <th scope="col">Unit ID</th>
                    <th scope="col">Reg Number</th>
                    <th scope="col">IMEI</th>
                    <th scope="col">MSISDN</th>
                    <th scope="col">Model</th>
                    <th scope="col">Date Created</th>

                </tr>
            </thead>
            <tfoot>

            </tfoot>
            <tbody>
                <% data["list"].forEach(function(tracker){%>
                    <tr>
                        <th data-title="ID">
                            <%=tracker["id"]%>
                        </th>
                        <td data-title="REG">
                            <%=tracker["label"]%>
                        </td>
                        <td data-title="IMEI">
                            <%=tracker["source"]["device_id"]%>
                        </td>
                        <td data-title="MSISDN" data-type="currency">
                            <%=tracker["source"]["phone"]%>
                        </td>
                        <td data-title="MODEL" data-type="currency">
                            <%=tracker["source"]["model"]%>
                        </td>
                        <td data-title="CREATED" data-type="currency">
                            <%=tracker["source"]["creation_date"]%>
                        </td>

                    </tr>
                    <%})%>




            </tbody>
        </table>
    </div>


    <div class="col-md-3 offset-md-3 my-5">
        <div class="card">
            <div class="card-body">
                <div>
                    <span> <form action="/detailed" method="GET">
                <div>
                    <label for="mySearch">  </label>
                    <!-- <input type="text" id="mySearch" name="userid" placeholder="Enter User API id" required size="40" minlength="20" maxlength="40"> -->
                   <div><input type="submit" class ="btn btn-primary btn-large" value="Click to Get Current Units Status.."></div> 
                    <span class="validity"></span>
                </div>
                </form>
                </span>
            </div>

            <!-- <div><a href="">Download CSV </a></div> -->
            <div><a href="/search">New Search </a></div>
        </div>

    </div>
    </div>
    </div>


    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/chart.js@2.9.4/dist/Chart.min.js"></script>
    <script type="text/javascript" src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.10.23/js/jquery.dataTables.js"></script>
    <script type="text/javascript" src="//cdn.datatables.net/buttons/1.6.5/js/dataTables.buttons.min.js"></script>
    <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
    <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js"></script>
    <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js"></script>
    <script type="text/javascript" src="//cdn.datatables.net/buttons/1.6.5/js/buttons.html5.min.js"></script>
    <script type="text/javascript" src="//cdn.datatables.net/buttons/1.6.5/js/buttons.print.min.js"></script>
    <script type="text/javascript" src="//cdn.datatables.net/buttons/1.6.5/js/buttons.colVis.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function() {
            $('#IDsTable').DataTable({
                "paging": false,
                "ordering": true,
                scrollY: '70vh',
                scrollCollapse: true,
                dom: 'Bfrtip',
                buttons: [
                    'copy',
                    'excel',
                    'csv',
                    'pdf',
                    'colvis'
                ]
            });
        });
    </script>

</body>

</html>